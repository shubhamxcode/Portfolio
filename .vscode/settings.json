{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "css.customData": [".vscode/css_custom_data.json"],
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  },
  "tailwindCSS.experimental.classRegex": [
    "class[:]\\s*?[\"'`]([^\"'`]*).*?[\"'`]",
    "className[:]\\s*?[\"'`]([^\"'`]*).*?[\"'`]",
    "\\btw['\"`]([^'\"`]*)['\"`]"
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  }
} 